{
  "name": "DataOverStreams",
  "version": "1.0.0",
  "description": "A simple project to upload a large csv to s3 and consume the file using javascript streams.",
  "main": "src/server/index.ts",
  "scripts": {
    "dev:server": "ts-node -r tsconfig-paths/register src/server/index.ts",
    "test": "npx jest --passWithNoTests --silent --noStackTrace --runInBand",
    "build": "tsc",
    "dry": "npx jscpd --config .jscpd.json",
    "linter": "npx prettier --check . && npx eslint . --ext ts --max-warnings 0",
    "prettier": "npx prettier -w ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/isaacmirandacampos/upload-of-bulk-data.git"
  },
  "keywords": [
    "node.js",
    "dynamodb",
    "s3",
    "typescript",
    "terraform",
    "localstack"
  ],
  "author": "Isaac de Miranda Campos",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/isaacmirandacampos/upload-of-bulk-data/issues"
  },
  "homepage": "https://github.com/isaacmirandacampos/upload-of-bulk-data#readme",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.8",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "jscpd": "^3.5.10",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.501.0",
    "@aws-sdk/s3-request-presigner": "^3.501.0"
  }
}
